{
  "name": "citysites/yii2-zend-rss",
  "description": "Yii2 Framework extension to provide functionality for consuming RSS and Atom feeds using zend-feed library.",
  "keywords": [
    "yii2",
    "extension",
    "rss",
    "atom",
    "feed"
  ],
  "type": "yii2-extension",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Ivan Shaposhnikov",
      "email": "haposhnikoff2008@ukr.net"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "^2.0",
    "laminas/laminas-feed": "2.5.*@dev",
    "laminas/laminas-servicemanager": "2.5.*@dev",
    "laminas/laminas-http": "2.5.*@dev"
  },
  "autoload": {
    "psr-0": {
      "yii\\feed\\": ""
    }
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
